{
  "name": "react-hybris",
  "description": "Proof of concept that react is compatible with hybris.",
  "version": "0.1.0",
  "author": "Daniel Radezky",
  "dependencies": {
    "bootstrap": "4.4.1",
    "gatsby": "^2.18.12",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-alias-imports": "1.0.5",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-sharp": "^2.3.7",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hook-form": "4.4.3"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0"
  },
  "keywords": [
    "react",
    "hybris",
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "lint": "eslint src",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run lint -- --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
